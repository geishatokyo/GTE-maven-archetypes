<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="trade"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  
  <modules>
    <module id="${rootArtifactId}-core_${scala.version}" dir="core" name="${rootArtifactId}-core_${scala.version}">
      <fileSets>
        <fileSet encoding="UTF-8">
          <directory>src/main/scala</directory>
          <includes>
            <include>**/*.scala</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src/test/scala</directory>
          <includes>
            <include>**/*.scala</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module id="${rootArtifactId}-mp-server" dir="mpserver" name="${rootArtifactId}-mp-server">
      <fileSets>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>src/main/extra-res</directory>
          <includes>
            <include>**/*.xml</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src/main/scala</directory>
          <includes>
            <include>**/*.scala</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src/test/scala</directory>
          <includes>
            <include>**/*.scala</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module id="${rootArtifactId}-mp-client" dir="mpclient" name="${rootArtifactId}-mp-client">
      <fileSets>
        <fileSet encoding="UTF-8">
          <directory>src/main/scala</directory>
          <includes>
            <include>**/*.scala</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src/test/scala</directory>
          <includes>
            <include>**/*.scala</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
  </modules>
</archetype-descriptor>
